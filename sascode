proc sql;
   /* Count unique IDs with PASS or FAIL */
   select count(distinct ID) into :total_count
   from your_dataset
   where not missing(Status) and Status in ('PASS', 'FAIL');

   /* Count unique IDs with PASS */
   select count(distinct ID) into :pass_count
   from your_dataset
   where Status = 'PASS';

   /* Calculate the percentage */
   select (&pass_count / &total_count) * 100 into :pass_percentage
   from dual; /* Dual is a dummy table used in many SQL dialects */

   /* Print the result */
   select "Percentage of PASS" as Label, &pass_percentage as Value
   from dual;
quit;
