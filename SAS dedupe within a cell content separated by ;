data z;
    input name $50.;
    datalines;
    John, Doe; Jane, Estes; John, Doe; Jane, Estes
    ;
run;

data z_unique;
    set z;
    length unique_name $50;
    retain unique_name;
    do i = 1 to countw(name, ';');
        temp_name = scan(name, i, ';');
        if index(unique_name, trim(temp_name)) = 0 then
            unique_name = catx('; ', unique_name, temp_name);
    end;
    drop i temp_name;
run;
